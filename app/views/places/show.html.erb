
<h1>
    <%= @place["name"] %>
</h1>

<ul>
<% for post in @posts %>
    <h2>
        <%=post["title"]%>
    </h2>
    <p><b>
        <%=post["posted_on"]%>
    </p></b>
    <p>
        <%=post["description"]%>
    </p>
<%end %>
</ul>



<p>
    <a href="/posts/new?place_id=<%= @place["id"]%>"> Create a new post for <%= @place["name"]%> </a>
</p>

<p>
<a href="/places/"> Go back to Places main page </a>
</p>

